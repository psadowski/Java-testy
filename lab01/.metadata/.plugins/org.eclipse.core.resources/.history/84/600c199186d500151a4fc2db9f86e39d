package com.example.calc;

public class Calculator {
	
	public int add(int n1, int n2){
		return n1 + n2;
	}
	
	public int sub(int n1, int n2){
		return n1 - n2;
	}
	
	public int mult(int n1, int n2){
		return n1 * n2;
	}
	
	public int div(int n1, int n2){
		return n1 / n2;
	}
	
	public boolean greater(int a, int b){
		return a > b;
	}
	
}
